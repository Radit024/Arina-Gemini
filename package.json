{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client:wait\"",
    "dev:client": "cd client && npm run dev",
    "dev:client:replit": "npm --prefix client run dev",
    "dev:client:wait": "wait-on tcp:5000 && npm run dev:client",
    "dev:server": "cd server && npm run dev:server",
    "dev:replit": "concurrently \"npm run dev:server\" \"npm run dev:client:replit\"",
    "build:client": "cd client && npm install && npm run build",
    "build:server": "cd server && npm install && npm run build:server",
    "build": "npm run build:client && npm run build:server",
    "build:ts": "tsc --project ./server/tsconfig.json",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push"
  },
